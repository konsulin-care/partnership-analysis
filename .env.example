# GENERAL
APP_NAME=partnership_analysis_system
ENVIRONMENT=production
DEBUG=false

# API KEYS
CARBONE_API_KEY=your_carbone_api_key_here
GOOGLE_GENAI_API_KEY=your_search_api_key

# RESEARCH CONFIGURATION
RESEARCH_MAX_QUERIES_PER_GAP=3
RESEARCH_CACHE_TTL_DAYS=30
RESEARCH_CONFIDENCE_THRESHOLD=0.70

# DATA EXTRACTION
EXTRACTION_CONFIDENCE_THRESHOLD=0.75
EXTRACTION_FLAG_LOW_CONFIDENCE=true

# FINANCIAL MODELING
FINANCIAL_DISCOUNT_RATE=0.10
FINANCIAL_CAPEX_AMORTIZATION_MONTHS=48
FINANCIAL_SENSITIVITY_VARIANCE=0.20

# FILE PATHS
INPUT_DIR=./inputs
OUTPUT_DIR=./outputs
CACHE_DIR=./cache
TEMPLATES_DIR=./templates
LOGS_DIR=./logs

# CACHE
CACHE_BACKEND=json_file
CACHE_FILE_PATH=./cache/research_cache.json
CACHE_AUTO_CLEANUP_DAYS=90

# LOGGING
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_FILE=./logs/execution.log

# SCHEMA and VALIDATION
SCHEMA_VERSION=1.0
SCHEMA_STRICT_MODE=true

# BRAND TO RESEARCH ABOUT
BRAND_NAME="Name of the brand brand, e.g., Konsulin"
BRAND_ABOUT="Define the scope of brand's business, e.g., providing booking management system through WhatsApp"
BRAND_ADDRESS="Brand's address"
BRAND_INDUSTRY="Industrial sector, e.g., IT Service"

# Hub information
HUB_NAME="Nama Kala"
HUB_LOCATION="BSD City, Tangerang"

# Deep research configuration
DEEP_RESEARCH_MAX_ITERATIONS=3
DEEP_RESEARCH_MODEL_SEARCH=gemini-2.0-flash
DEEP_RESEARCH_MODEL_SYNTHESIS=gemini-2.5-flash
DEEP_RESEARCH_MODEL_QUESTIONS=gemini-2.5-flash
DEEP_RESEARCH_ITERATION_TIMEOUT=300
DEEP_RESEARCH_CACHE_TTL_DAYS=7
DEEP_RESEARCH_GAP_THRESHOLD=3
