# GENERAL
APP_NAME=partnership_analysis_system
ENVIRONMENT=production
DEBUG=false

# RESEARCH CONFIGURATION
RESEARCH_WEB_SEARCH_ENABLED=true
RESEARCH_MAX_QUERIES_PER_GAP=3
RESEARCH_CACHE_TTL_DAYS=30
RESEARCH_CONFIDENCE_THRESHOLD=0.70

# DATA EXTRACTION
EXTRACTION_CONFIDENCE_THRESHOLD=0.75
EXTRACTION_FLAG_LOW_CONFIDENCE=true

# FINANCIAL MODELING
FINANCIAL_DISCOUNT_RATE=0.10
FINANCIAL_CAPEX_AMORTIZATION_MONTHS=48
FINANCIAL_SENSITIVITY_VARIANCE=0.20

# CARBONE SDK
CARBONE_API_KEY=your_carbone_api_key_here
CARBONE_API_VERSION=5

# FILE PATHS
INPUT_DIR=./inputs
OUTPUT_DIR=./outputs
CACHE_DIR=./cache
TEMPLATES_DIR=./templates
LOGS_DIR=./logs

# CACHE
CACHE_BACKEND=json_file
CACHE_FILE_PATH=./cache/research_cache.json
CACHE_AUTO_CLEANUP_DAYS=90

# LOGGING
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_FILE=./logs/execution.log

# WEB SEARCH API
WEB_SEARCH_API_KEY=your_search_api_key
WEB_SEARCH_PROVIDER=custom_search

# SCHEMA and VALIDATION
SCHEMA_VERSION=1.0
SCHEMA_STRICT_MODE=true

# NOTIFICATIONS
NOTIFICATION_EMAIL_ON_COMPLETION=true
NOTIFICATION_EMAIL_ADDRESS=user@example.com
NOTIFICATION_SLACK_WEBHOOK=https://hooks.slack.com/...
